{
  "multiline comment": {
    "prefix": "/**",
    "body": ["/**\n * ${0}\n */"],
    "description": "multiline comment"
  },
  "vue": {
    "prefix": "vue",
    "body": [
      "<script setup lang=\"ts\">",
      "/**",
      " * $TM_FILENAME_BASE",
      " *",
      " * ${1:Description of component...}",
      " */",
      "",
      "${0}",
      "</script>",
      "",
      "<script lang=\"ts\">",
      "export default {",
      "\tname: '$TM_FILENAME_BASE',",
      "}",
      "</script>",
      "",
      "<template>",
      "\t<div></div>",
      "</template>",
      "",
      "<style lang=\"sass\">",
      "</style>"
    ],
    "description": "Vue SFC Scaffold (Composition API, Typescript)"
  }
}
